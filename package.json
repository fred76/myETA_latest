{
    "name": "eta",
    "version": "1.0.0",
    "description": "ETA & ROB",
    "main": "dist/electron/electron/electron-main.js",
    "author": "Alberto Lunardini",
    "license": "MIT",
    "build": {
        "npmRebuild": true,
        "appId": "L5EWQJU24M.com.fred76.embedded",
        "extends": null,
        "files": [
            "dist/**/*",
            "package.json",
            "embeddedprovisionprofile.provisionprofile",
            "entitlements.mac.plist",
            "Certificati.p12",
            "notarize.js"
        ],
        "mac": {
            "category": "public.app-category.productivity",
            "entitlements": "./entitlements.mac.plist",
            "entitlementsInherit": "./entitlements.mac.plist",
            "hardenedRuntime": true,
            "provisioningProfile": "embeddedprovisionprofile.provisionprofile",
            "target": [
                "zip"
            ]
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64"
                    ]
                }
            ]
        },
        "nsis": {
            "oneClick": false,
            "perMachine": false,
            "allowElevation": true,
            "allowToChangeInstallationDirectory": true
        }
    },
    "resolutions": {
        "node-sass": "^6.0.1"
    },
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "ngbuild": "ng build  --c=production --base-href ./ ",
        "test": "ng build --base-href ./  && tsc --p electron && electron . --c=production",
        "build:prod": "npm run build --base-href -- -c production",
        "electron:mac": "electron-builder build --mac",
        "electron:win": "yarn build:prod && npx electron-builder build -w ",
        "builder-mac": "electron-builder build --mac",
        "builder-win": "electron-builder build --x64 --windows",
        "builder-win-portable": "electron-builder build --x64 --windows portable",
        "postinstall": "electron-builder install-app-deps",
        "b": "electron-builder",
        "mygit": "git push && git push --tags"
    },
    "private": true,
    "dependencies": {
        "@angular-material-components/datetime-picker": "^8.0.0",
        "@angular/animations": "^14.0.0",
        "@angular/cdk": "^14.2.4",
        "@angular/common": "^14.0.0",
        "@angular/compiler": "^14.0.0",
        "@angular/core": "^14.0.0",
        "@angular/flex-layout": "^14.0.0-beta.40",
        "@angular/forms": "^14.0.0",
        "@angular/material": "^14.2.4",
        "@angular/platform-browser": "^14.0.0",
        "@angular/platform-browser-dynamic": "^14.0.0",
        "@angular/router": "^14.0.0",
        "@fontsource/material-icons": "^4.5.4",
        "@ngneat/until-destroy": "^9.2.2",
        "date-fns": "^2.29.3",
        "date-fns-tz": "^1.3.7",
        "electron-reloader": "^1.2.3",
        "file-saver": "^2.0.5",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.25",
        "lodash": "^4.17.21",
        "lodash.clonedeep": "^4.5.0",
        "material-icons": "^1.12.0",
        "ngx-date-fns": "^10.0.0",
        "ngx-electronify": "^2.0.1",
        "node-gyp": "^9.3.0",
        "roboto-fontface": "^0.10.0",
        "rxjs": "~7.5.0",
        "sqlite3": "^5.1.2",
        "tslib": "^2.3.0",
        "typeorm": "^0.3.10",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.2.5",
        "@angular/cli": "~14.0.1",
        "@angular/compiler-cli": "^14.0.0",
        "@types/electron": "^1.6.10",
        "@types/file-saver": "^2.0.5",
        "@types/jasmine": "~4.0.0",
        "@types/node": "^18.11.9",
        "concurrently": "^7.4.0",
        "electron": "^21.1.0",
        "electron-builder": "^22.10.3",
        "electron-installer-dmg": "^4.0.0",
        "electron-installer-windows": "^3.0.0",
        "electron-rebuild": "^3.2.9",
        "jasmine-core": "~4.1.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "typescript": "~4.7.2",
        "wait-on": "^6.0.1"
    }
}
